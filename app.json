{
  "expo": {
    "name": "TennisCue",
    "slug": "TennisCue",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tenniscue",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C853"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tenniscue.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "TennisCue needs camera access to record your tennis sessions.",
        "NSMicrophoneUsageDescription": "TennisCue needs microphone access to record audio during sessions.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#00C853"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tenniscue.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1ffa61f5-f54d-45a3-9fe4-b27a4d2aa7af"
      }
    }
  }
}
